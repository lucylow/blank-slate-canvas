{
  "name": "pitwall-agents",
  "version": "1.0.0",
  "description": "Multi-agent orchestration system for PitWall AI",
  "scripts": {
    "orchestrator": "node orchestrator/router.js",
    "orchestrator:simple": "node orchestrator/index.js",
    "preprocessor": "node preprocessor/preprocessor-agent.js",
    "preprocessor:simple": "node preprocessor/index.js",
    "delivery": "node delivery/delivery-agent.js",
    "delivery:simple": "node delivery/index.js",
    "start:all": "concurrently \"npm:orchestrator\" \"npm:preprocessor\" \"npm:delivery\"",
    "start:simple": "concurrently \"npm:orchestrator:simple\" \"npm:preprocessor:simple\" \"npm:delivery:simple\""
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}

